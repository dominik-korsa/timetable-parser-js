  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/.bin/eslint ./lib/ ./test/ --ext .js,.ts",
    "lint:fix": "./node_modules/.bin/eslint ./lib/ ./test/ --ext .js,.ts --fix",
